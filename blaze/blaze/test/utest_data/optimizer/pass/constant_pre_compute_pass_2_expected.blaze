name : "batch_normalization_net"
run_mode : "simple"
device_option {
  device_type : 0
  device_id : 0
}

op {
  name : "constant_fill"
  type : "ConstantFill"
  arg {
    name : "shape"
    ints : 2
    ints : 3
  }
  arg {
    name : "dtype"
    i : 1
  }
  arg {
    name : "value"
    floats : 0.1
    floats : 0.1
    floats : 0.1
    floats : 0.1
    floats : 0.2
    floats : 0.3
  }
  output : "att_comm"
}

op {
  name : "gamma"
  type : "ConstantFill"
  arg {
    name : "shape"
    ints : 3
  }
  arg {
    name : "dtype"
    i : 1
  }
  arg {
    name : "value"
    floats : 0.01
    floats : 0.02
    floats : 0.03
  }
  output : "gamma"
}

op {
  name : "beta"
  type : "ConstantFill"
  arg {
    name : "shape"
    ints : 3
  }
  arg {
    name : "dtype"
    i : 1
  }
  arg {
    name : "value"
    floats : 0.03
    floats : 0.04
    floats : 0.05
  }
  output : "beta"
}

op {
  name : "mean"
  type : "ConstantFill"
  arg {
    name : "shape"
    ints : 3
  }
  arg {
    name : "dtype"
    i : 1
  }
  arg {
    name : "value"
    floats : 0.09
    floats : 0.08
    floats : 0.07
  }
  output : "mean"
}

op {
  name : "var"
  type : "ConstantFill"
  arg {
    name : "shape"
    ints : 3
  }
  arg {
    name : "dtype"
    i : 1
  }
  arg {
    name : "value"
    floats : 0.033166247903554
    floats : 0.03605551275463989
    floats : 0.03741657386773942
  }
  output : "var"
}

op {
  name : "batch_normalization"
  type : "BatchNormalization"
  input : "att_comm"
  input : "gamma"
  input : "beta"
  input : "mean"
  input : "var"
  output : "output"
  arg {
    name : "nosqrt"
    i : 1
  }
}

# The output check
external_output {
  name : "output"
  dtype : 1
}
arg {
  name : "output_shape"
  ints : 2
  ints : 3
}
arg {
  name : "output"
  floats : 0.03301511 
  floats : 0.051094
  floats : 0.07405351
  floats : 0.03301511
  floats : 0.10656402
  floats : 0.23441026
}
